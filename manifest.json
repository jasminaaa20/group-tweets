{
  "manifest_version": 3,
  "name": "Group Tweets",
  "version": "1.0",
  "description": "Add tweets to custom groups on X.",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["*://x.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
